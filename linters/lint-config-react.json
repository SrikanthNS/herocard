{
  "extends": [
    "airbnb",
    "plugin:react/recommended",
    "plugin:jasmine/recommended",
    "plugin:lodash/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "jasmine", "react", "lodash"
  ],
  "env": {
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true,
    "amd": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "blockBindings": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "$": true,
    "__dirname": true,
    "analytics": true,
    "angular": true,
    "console": true,
    "jasmine": true,
    "spyOn": true,
    "userSpy": true,
    "beforeEach": true,
    "afterEach": true,
    "describe": true,
    "xdescribe": true,
    "expect": true,
    "it": true,
    "xit": true,
    "inject": true,
    "module": true,
    "require": true,
    "confirm": true,
    "formatLocal": true,
    "scope": true,
    "defaultHeaders": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "linebreak-style": ["error", "windows"],
    "consistent-return": "off",
    "curly": [
      "error",
      "all"
    ],
    "default-case": "off",
    "dot-notation": "error",
    "eqeqeq": [
      "error",
      "allow-null"
    ],
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-named-as-default": "off",
    "import/no-named-as-default-member": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/label-has-for": "off",
    "react/display-name": "off",
    "lodash/prefer-noop": "off",
    "lodash/prop-shorthand": "off",
    "lodash/collection-method-value": "off",
    "lodash/collection-return": "off",
    "lodash/matches-prop-shorthand": "off",
    "lodash/prefer-constant": "off",
    "lodash/prefer-reject": "off",
    "jasmine/no-spec-dupes": "off",
    "lodash/prefer-lodash-method": [2, {"ignoreMethods": ["find", "filter"]}],
    "lodash/import-scope": [2, "full"],
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreUrls": true
      }
    ],
    "new-cap": "error",
    "no-caller": "error",
    "no-cond-assign": [
      "error",
      "except-parens"
    ],
    "no-console": "error",
    "no-constant-condition": "off",
    "no-dupe-keys": "error",
    "no-debugger": "error",
    "no-empty": "error",
    "no-eval": "error",
    "no-extend-native": "error",
    "no-irregular-whitespace": "error",
    "no-iterator": "error",
    "no-loop-func": "error",
    "no-new": "error",
    "no-plusplus": "off",
    "no-proto": "error",
    "no-script-url": "error",
    "no-sequences": "error",
    "no-with": "error",
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "react/jsx-filename-extension": "off",
    "react/no-unescaped-entities": "off",
    "react/react-in-jsx-scope": "error",
    "semi": "error",
    "spaced-comment": [
      "error",
      "always",
      {
        "exceptions": ["/"]
      }
    ],
    "valid-typeof": "error",
    "no-underscore-dangle": ["error", {
      "allow": ["_delete"]
    }]
  }
}
